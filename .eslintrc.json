{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["error", "always"],
    "eol-last": ["error", "always"],
    "indent": ["error", 2],
    "object-curly-spacing":  ["error", "always"],
    "array-bracket-spacing": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "no-trailing-spaces": ["error"],
    "quotes": ["error", "single"],
    "key-spacing": "error",
    "comma-spacing": "error"
  }
}
